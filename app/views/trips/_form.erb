

<%= form_for trip do |f| %>
    <%= f.label :destination %>
    <%= f.text_field :destination %> <br>
    <%= f.label :pickup %>
    <%= f.datetime_field :pickup %> <br>
    <%= f.label :dropoff %>
    <%= f.datetime_field :dropoff %> <br>
    <%= f.label "Please select a Gear List you'd like to rent:" %>
    <%= f.select :gear_list_id, @gear_lists.collect{|g| [g.name, g.id]} %> <br>
    
    <%= f.submit %> 
<% end %>